#include<stdio.h>

void main()

{
    int a,b,c,d;
    
    printf("Enter first value = ");
    scanf("%d",&a);
    
    printf("Enter second value = ");
    scanf("%d",&b);
    
    printf("Enter third value = ");
    scanf("%d",&c);
    
    printf("Before swapping a=%d and b=%d and c=%d\n",a,b,c);
    
    d = a;
    a = b;
    b = c;
    c = d;
    
    printf("After swapping a=%d and b=%d and c=%d ",a,b,c);
    
}
